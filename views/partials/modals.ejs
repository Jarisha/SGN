<!-- Log In Modal -->
<div class="modal hide" id="loginModal" >
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
    <h3 id="myModalLabel">Log in to Quyay</h3>
  </div>
  <div class="modal-body">
    <form id="loginForm">
