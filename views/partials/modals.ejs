        <label for="register_email" >Email</label>
        <input id="register_email" ng-model="register.email" type="email" ></input>
      </div>
      <div class="modal_stack edit_field">
        <label for="register_username" >Username</label>
        <input id="register_username" ng-model="register.name" type="text" ></input>
      </div>
      <div class="modal_stack edit_field">
        <label for="register_password" >Password</label>
        <input id="register_password" ng-model="register.password" type="password" ></input>
      </div>
      <div class="modal_stack edit_field">
        <label for="register_confirm" >Confirm</label>
        <input id="register_confirm" ng-model="register.confirm" type="password" ></input>
      </div>
      <div class="modal_stack edit_field">
        <button class="q_register_btn" ng-click="registerSubmit()">Register</button>
      </div>
    </form>
  </div>
</div>

<!-- Register Step 2: Upload Photo -->
<div class="modal hide" id="registerModal_2" >
  <div class="modal-header">
