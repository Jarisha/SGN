<div ng-repeat="pin in dummyData" class="store_pin" masonry>
  <div class="store_container">
    <p class="game_name"><b>{{pin.name}}</b></p>
  </div>
  <a class="view_trigger" data-toggle="modal" data-dynamic="true">
    <img class="game_img" ng-src="{{pin.image}}" width="200"/>
  </a>
</div>
